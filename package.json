    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "multer": "^1.4.5-lts.1",
    "oauth": "^0.10.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.1",
    "supertest": "^6.3.3",
    "typedi": "^0.10.0",
    "typescript": "^4.9.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.16",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^29.2.3",
    "@types/multer": "^1.4.7",
    "@types/oauth": "^0.9.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-plugin-jest": "^27.1.6",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "pre-commit": "^1.2.2",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "yarn lint"
    ],
    "__tests__/**/*.ts": [
      "yarn lint"
    ]
